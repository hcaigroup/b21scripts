<launch>
        
    <node pkg="pr2_teleop" type="teleop_pr2_keyboard" name="spawn_teleop_keyboard" output="screen">
      <remap from="cmd_vel" to="/b21/cmd_vel" />

      <param name="walk_vel" value="0.2" />
      <param name="run_vel" value="0.3" />
      <param name="yaw_rate" value="0.3" />
      <param name="yaw_run_rate" value="0.4" />
    </node>
</launch>

